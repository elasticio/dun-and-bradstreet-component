	<!-- The type: com.dnb.dataservice.customproductservice.PCMDataService.ShareCapitalDetailsType -->
	<xs:element name="ShareCapitalDetailsType" type="cusprod:ShareCapitalDetailsType"/>
	<xs:complexType name="ShareCapitalDetailsType">
		<xs:annotation>
			<xs:documentation>
An entry loop which can repeat multiple times to allow the recording of the current Capital information.
         </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CapitalAmount" type="cusprod:AmountType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
The amount of the capital of this type. If figure is stored locally with 000s omitted then this must be multiplied and the full figure populated to the record, e.g., If Capital is 543 with 000s omitted then the figure of 543000 should be populated.
               </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StartDate" type="cusprod:DNBDateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
The date on which the Capital Amount was first registered.
               </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CapitalAmountUndisclosedReasonText" type="cusprod:DNBDecodedStringType" minOccurs="0"/>
			<xs:element name="OriginalCapitalIndicator" type="xs:boolean" minOccurs="0"/>
			<xs:element name="CapitalStockDetails" type="cusprod:CapitalStockDetails" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="CapitalTypeText" type="cusprod:DNBDecodedStringNewType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
Text that describes the type of capital.e.g., Issued, Paid in, Nominal/Authorized.
               </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="TypeText" type="cusprod:StringBaseType256">
			<xs:annotation>
				<xs:documentation>
Text that describes the type of the capital, e.g., Issued, Paid in, Nominal/Authorized.
            </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DNBCodeTableNumber" type="xs:int"/>
		<xs:attribute name="DNBCodeValue" type="cusprod:DNBCodeValueType"/>
	</xs:complexType>
