	<!-- The type: com.dnb.dataservice.customproductservice.PCMDataService.TradeDataAvailabilityDetail -->
	<xs:element name="TradeDataAvailabilityDetail" type="cusprod:TradeDataAvailabilityDetail"/>
	<xs:complexType name="TradeDataAvailabilityDetail">
		<xs:annotation>
			<xs:documentation>
Records the statistical information based on the trade payment data available for this organization.
         </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="TradeDataAvailableIndicator" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
When turned ON (set to TRUE) this indicates that D&amp;amp;B has business trading information on this organization.

When turned OFF (set to FALSE) this indicates that D&amp;amp;B does not have business trading information on this organization.
               </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AssessmentText" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
