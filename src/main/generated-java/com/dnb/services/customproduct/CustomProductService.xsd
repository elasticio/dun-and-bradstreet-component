	<!-- The type: com.dnb.dataservice.customproductservice.PCMDataService.CommercialCollectionClaimDetails -->
	<xs:element name="CommercialCollectionClaimDetails" type="cusprod:CommercialCollectionClaimDetails"/>
	<xs:complexType name="CommercialCollectionClaimDetails">
		<xs:sequence>
			<xs:element name="StartDate" type="cusprod:DNBDateType" minOccurs="0">
				<xs:annotation>
