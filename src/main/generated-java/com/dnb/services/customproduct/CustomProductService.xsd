			<xs:element name="ProfileChapterName" type="cusprod:IndustryChapterNameEnum" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>
Name identifying the major division of the business activity profile information.
               </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RequestedOrganizationChildLinkageSpecification" type="cusprod:RequestedOrganizationChildLinkageSpecification" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="LinkedOrganizationLocationSpecification" type="cusprod:LinkedOrganizationLocationSpecification" minOccurs="0">
