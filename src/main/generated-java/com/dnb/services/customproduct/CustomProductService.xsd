	<!-- The type: com.dnb.dataservice.customproductservice.PCMDataService.SpendBehaviorType -->
	<xs:element name="SpendBehaviorType" type="cusprod:SpendBehaviorType"/>
	<xs:complexType name="SpendBehaviorType">
		<xs:sequence>
			<xs:element name="LocationCount" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
The total number of distinct business sites identified for this company.
