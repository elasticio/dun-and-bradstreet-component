			<xs:element name="FilingEmployerIdentificationNumberDetail" type="cusprod:OrganizationIdentificationNumberSimpleType" minOccurs="0"/>
			<xs:element name="RemedyFiled" type="cusprod:RemedyFiled" minOccurs="0" maxOccurs="unbounded">
